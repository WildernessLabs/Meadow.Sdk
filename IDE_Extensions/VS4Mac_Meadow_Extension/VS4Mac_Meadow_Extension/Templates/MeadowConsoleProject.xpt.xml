<?xml version="1.0" encoding="UTF-8"?>
<Template originator="Adrian Stevens" created="7/21/2019" lastModified="7/21/2019">
	<!-- Template Header -->
	<TemplateConfiguration>
		<_Name>Meadow Console Application</_Name>
		<Category>C#/Meadow</Category>
        <LanguageName>C#</LanguageName>
		<_Description>Creates an empty C# Meadow console application project</_Description>
	</TemplateConfiguration>
	<!-- Actions -->
	<Actions>
	</Actions>
    <!-- Template Content -->
    <Combine name="${ProjectName}" directory=".">
        <Project name="${ProjectName}" directory="." type="Meadow">
        <Options TargetFrameworkVersion = "4.5" />
            <Files>
                <File name="Program.cs" BuildAction="Compile" />
                <Directory name="Media" />
            </Files>
        </Project>
    </Combine>
</Template>